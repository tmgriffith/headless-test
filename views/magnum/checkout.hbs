    <script>
      $(function() {

        var counter = 3000,

          cDisplay = document.getElementById("stopwatch");

        //console.log( counter );

        format = function(t) {

          var minutes = Math.floor(t / 600);

          var seconds = Math.floor((t / 10) % 60);

          var mil_sec = Math.floor(t % 10);

          minutes = (minutes < 10) ? "0" + minutes.toString() : minutes.toString();

          seconds = (seconds < 10) ? "0" + seconds.toString() : seconds.toString();

          mil_sec = (mil_sec < 10) ? "0" + mil_sec.toString() : mil_sec.toString();

          if (seconds == 0 && minutes == 0 && Math.floor(t % 10)) {
            cDisplay.innerHTML = "00:00:00";
            clearInterval(interval);
          } else {
            cDisplay.innerHTML = minutes + ":" + seconds + ":" + mil_sec;
          }

        };

        var interval = setInterval(function() {

          counter--;

          format(counter);

        }, 100);
      });

      $(function() {
        // Invoke the plugin
        $('input, textarea').placeholder({
          customClass: 'my-placeholder'
        });
      });
